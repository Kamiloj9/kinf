#include <iostream>
#include <string>
using namespace std;

int main()
{
    int n=20;
    int t[n];
    for(int i=0; i<n; i++)
    {
        cin >> t[i];
    }
    for(int i=0; i<n; i++)
    {
        if(t[i]%2 == 0)
        {
            cout << t[i] << " ";
        }
    }
    for(int i=0; i<n; i++)
    {
        if(t[i]%2 != 0)
        {
            cout << t[i] << " ";
        }
    }

}
